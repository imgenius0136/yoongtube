{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "quotes": "off",
    "no-unused-vars": "off",
    "object-curly-newline": "off",
    "comma-dangle": "off",
    "no-console": "off",
    "implicit-arrow-linebreak": "off",
    "no-undef": "off",
    "spaced-comment": "off",
    "arrow-parens": "off",
    "no-else-return": "off",
    "import/no-named-as-default": "off"
  }
}
